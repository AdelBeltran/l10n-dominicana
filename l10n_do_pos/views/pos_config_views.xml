<odoo>
    <record id="l10n_do_pos_pos_config_view_form" model="ir.ui.view">
        <field name="name">l10n.do.pos.config.view.form</field>
        <field name="model">pos.config</field>
        <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
        <field name="arch" type="xml">

            <xpath expr="//div[@id='iface_invoicing']//div[2]//div[2]">
                <div class="row mt16">
                    <label string="Default customer" for="default_partner_id" class="col-md-3 o_light_label"/>
                    <field name="default_partner_id"/>
                </div>
            </xpath>

        </field>
    </record>

</odoo>